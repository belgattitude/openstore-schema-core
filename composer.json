{
    "name": "openstore/schema-core",
    "description": "Openstore core schema",
    "license": "MIT",
    "keywords": [
        "openstore",
        "schema-core"
    ],
    "homepage": "https://github.com/belgattitude/openstore-schema-core",
    "type": "library",
    "authors": [
        {
            "name": "SÃ©bastien Vanvelthem",
            "homepage": "http://www.nuvolia.com/",
            "role": "Lead developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "OpenstoreSchema\\Core\\": "src/OpenstoreSchema/Core"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenstoreFixture\\Core\\": "fixtures/OpenstoreFixture/Core"
        }
    },

    "require": {
        "php": "^5.6 || ^7.0",
        "doctrine/dbal": "^2.5.9",
        "doctrine/common": "^2.6.1 || ^2.7.2",
        "doctrine/orm": "^2.5.6",
        "gedmo/doctrine-extensions": "^2.4.26",
        "doctrine/data-fixtures": "^1.2.2",
        "symfony/console": "^2.6.0 || ^3.0.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.1.0"
    },
    "bin": ["bin/openstore-schema-core"],
    "scripts": {
        "fix": [
            "@cs-fix"
        ],
        "cs-check": "vendor/bin/php-cs-fixer --diff --dry-run -v fix",
        "cs-fix": "vendor/bin/php-cs-fixer -v fix"
    },
    "archive": {
        "exclude": ["test", "doc", "tools", ".travis.yml", ".codeclimate.yml", ".coveralls.yml", ".scrutinizer.yml", "phpcs.xml", ".gitignore"]
    }
}


